      and "CSTATE IMAD T' 0 \<and> nextSnoopIs SnpData T' 0 \<longrightarrow> CSTATE ISAD T' 1"
      and "CSTATE IMAD T' 1 \<and> nextSnoopIs SnpData T' 1 \<longrightarrow> CSTATE ISAD T' 0"
      and "CSTATE IMD T' 0 \<and> nextSnoopIs SnpData T' 0 \<longrightarrow> HSTATE SAD T'"
      and "CSTATE IMD T' 1 \<and> nextSnoopIs SnpData T' 1 \<longrightarrow> HSTATE SAD T'"
      and "CSTATE IMA T' 0 \<and> nextSnoopIs SnpData T' 0 \<longrightarrow> HSTATE SAD T'"
      and "CSTATE IMA T' 1 \<and> nextSnoopIs SnpData T' 1 \<longrightarrow> HSTATE SAD T'"
