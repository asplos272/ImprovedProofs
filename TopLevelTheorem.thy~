      and "HSTATE SharedM T' \<and> nextReqIs CleanEvict T' 1 \<longrightarrow> \<not> CSTATE MIA T' 1"
      and "CSTATE Shared T' 0 \<and> nextSnoopIs SnpInv T' 0 \<longrightarrow> (CSTATE IMAD T' 1 \<or> CSTATE SMAD T' 1) \<and> htddatas2 T' \<noteq> [] \<or> (CSTATE IMA T' 1 \<or> CSTATE SMA T' 1) \<and> htddatas2 T' = []"
      and "CSTATE Shared T' 1 \<and> nextSnoopIs SnpInv T' 1 \<longrightarrow> (CSTATE IMAD T' 0 \<or> CSTATE SMAD T' 0) \<and> htddatas1 T' \<noteq> [] \<or> (CSTATE IMA T' 0 \<or> CSTATE SMA T' 0) \<and> htddatas1 T' = []"
      and "nextReqIs DirtyEvict T' 0 \<longrightarrow> nextEvict T' 0"
