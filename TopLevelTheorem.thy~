      and "(HSTATE SAD T' \<or> HSTATE MAD T' \<or> HSTATE SA T' \<or> HSTATE MA T') \<and> snpresps1 T' \<noteq> [] \<longrightarrow> htddatas1 T' = [] \<or> CSTATE ISDI T' 0"
      and "(HSTATE SAD T' \<or> HSTATE MAD T' \<or> HSTATE SA T' \<or> HSTATE MA T') \<and> snpresps2 T' \<noteq> [] \<longrightarrow> htddatas2 T' = [] \<or> CSTATE ISDI T' 1"
      and "nextSnpRespIs RspSFwdM T' 0 \<longrightarrow> CSTATE Shared T' 0 \<or> CSTATE SMAD T' 0 \<or> CSTATE SIA T' 0 \<or> CSTATE SIAC T' 0"
