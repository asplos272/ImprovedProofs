      and "HSTATE MAD T' \<and> nextDTHDataFrom 0 T' \<longrightarrow> CSTATE IMAD T' 1"
      and "HSTATE MAD T' \<and> nextDTHDataFrom 1 T' \<longrightarrow> CSTATE IMAD T' 0"
      and "HSTATE MAD T' \<and> nextDTHDataFrom 0 T' \<longrightarrow> snpresps2 T' = []"
      and "HSTATE MAD T' \<and> nextDTHDataFrom 1 T' \<longrightarrow> snpresps1 T' = []"
      and "HSTATE MAD T' \<and> nextDTHDataFrom 0 T' \<longrightarrow> \<not> nextReqIs CleanEvict T' 0 \<and> \<not> nextReqIs CleanEvictNoData T' 0"
