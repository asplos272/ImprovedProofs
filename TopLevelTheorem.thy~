      and "snpresps1 T' \<noteq> [] \<longrightarrow> reqresps2 T' = []"
      and "snpresps2 T' \<noteq> [] \<longrightarrow> reqresps1 T' = []"
      and "length (snpresps1 T') \<le> 1"
      and "length (snpresps2 T') \<le> 1"
      and "HSTATE SAD T' \<and> (nextSnpRespIs RspIFwdM T' 0 \<or> nextSnpRespIs RspSFwdM T' 0) \<longrightarrow> CSTATE ISAD T' 1"
